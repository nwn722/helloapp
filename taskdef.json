{
    "executionRoleArn": "arn:aws:iam::715451173743:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "helloapp-container",
            "image": "715451173743.dkr.ecr.ap-northeast-1.amazonaws.com/helloapp",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "ecs-demo"
}